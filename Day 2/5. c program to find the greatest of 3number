#include<stdio.h>
int main()
 {
    int a,b,c;
    printf("Enter any number");
    scanf("%d %d %d", &a,&b, &c);
    if(a>b && a>c)
    {
       printf("A is bigger");
    }  
    else if(b>a && b>c)
    {
       printf("B is bigger");
    }
      else
    {
       printf("C is bigger");
    }
  return 0;
}
